                  <p class="help-block muted">
                    <span class="muted">i.e. http://payparrot.com/</span>
                  </p>
                </div>
              </div>                                       
              <div class="form-actions">
                <button type="submit" class="btn btn-success">Next step</button>
              </div>
            </fieldset>
